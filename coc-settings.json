{
  "eslint.enable": true,
  "python.formatting.blackArgs": [],
  "python.formatting.blackPath": "black",
  "python.jediEnabled": true,
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.formatOnSaveFiletypes": ["typescript", "typescriptreact", "javascript", "javascriptreact"],
  "list.nextKeymap": "<C-n>",
  "list.previousKeymap": "<C-p>",
  "list.maxHeight": 15,
  "list.insertMappings": {
    "<C-c>": "do:exit"
  },
  "languageserver": {
    "godot": {
      "host": "127.0.0.1",
      "filetypes": ["gd", "gdscript3"],
      "port": 6008
    }
  },
  "prettier.disableSuccessMessage": true,
  "tsserver.formatOnType": false,
  "typescript.format.enabled": false,
  "prettier.formatterPriority": 1,
  "suggest.snippetIndicator": "►",
  "suggest.minTriggerInputLength": 2,
  "diagnostic.virtualText": false,
  "diagnostic.locationlist": true,
  "diagnostic.enableMessage": "always",
  "diagnostic.virtualTextPrefix": " ‣ ",
  "eslint.filetypes": [
    "typescript",
    "typescriptreact",
    "javascript",
    "javascriptreact"
  ]
}
